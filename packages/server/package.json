{
  "name": "@multicast/server",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --T src/server.ts",
    "dev:queue": "ts-node-dev -r tsconfig-paths/register src/queue.ts",
    "test": "jest",

    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:save": "prisma migrate save --experimental",
    "prisma:down": "prisma migrate down --experimental",
    "prisma:up": "prisma migrate up --experimental",
    "prisma:studio": "prisma migrate studio --experimental"
  },
  "dependencies": {
    "@prisma/client": "^2.6.2",
    "bull": "^3.18.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.1.0",
    "extenso": "^2.0.1",
    "generate-password": "^1.5.1",
    "handlebars": "^4.7.6",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.5",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "oracledb": "^5.0.0",
    "puppeteer": "^5.3.0",
    "uuid": "^8.3.0",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@prisma/cli": "^2.6.2",
    "@types/bull": "^3.14.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/express-handlebars": "^3.1.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.161",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/multer": "^1.4.4",
    "@types/nodemailer": "^6.4.0",
    "@types/puppeteer": "^3.0.2",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.7",
    "npm-run-all": "^4.1.5",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  }
}
